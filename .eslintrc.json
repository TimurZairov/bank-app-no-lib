{
	"env": { "browser": true, "node": true, "es2021": true },
	"extends": "eslint:recommended",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"semi": "off",
		"arrow-parens": "off",
		"comma-dangle": "off",
		"require-jsdoc": "off",
		"no-tabs": "off"
	},
	"settings": {
		"import-resolver": {
			"alias": {
				"map": [["@", "./app"]]
			},
			"extensions:": [".js"]
		}
	}
}
